    "b = float(input(\"Enter marks for Subject 2: \"))\n",
    "c = float(input(\"Enter marks for Subject 3: \"))\n",
    "avg = (a + b + c) / 3\n",
    "if avg >= 90:\n",
    "    result = \"A\"\n",
    "elif 80 <= avg < 90:\n",
    "    result = \"B\"\n",
    "elif 60 <= avg < 70:\n",
    "    result = \"C\"\n",
    "else:\n",
    "    result = \"Fail\"\n",
